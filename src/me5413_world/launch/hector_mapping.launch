<launch>

  <!-- Connect the robot to a keyboard teleop controller -->
  <node name="teleop_twist_keyboard" pkg="teleop_twist_keyboard" type="teleop_twist_keyboard.py" output="screen" respawn="true"/>
  
  <include file="$(find me5413_world)/launch/hector_mapping_default.launch"/>

  
  <include file="$(find me5413_world)/hector_geotiff_launch/launch/geotiff_mapper.launch">
    <arg name="trajectory_source_frame_name" value="scanmatcher_frame"/>
    <arg name="map_file_path" value="$(find me5413_world)/maps"/>
  </include>
  
    <!--node name="recordbag" pkg="rosbag" type="record" 
      args="-a -o /home/atharva/hectorslam_new.bag" /-->
  
  <!-- Launch Rviz with our settings -->
  <node type="rviz" name="rviz" pkg="rviz" args="-d $(find me5413_world)/rviz/gmapping.rviz" output="log" respawn="true"/>

</launch>
